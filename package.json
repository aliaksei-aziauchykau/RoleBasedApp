{
    "name": "market",
    "version": "1.0.0",
    "engines": {
        "node": ">=11.3.0",
        "yarn": ">=1.12.3"
    },
    "scripts": {
        "------UI-----": "UI Scripts",
        "ui:build": "webpack --config webpack.config.js",
        "ui:watch": "webpack --config webpack.config.js --watch",
        "ui:watch:prod": "SET CONFIG_PATH='./env/.prod.env' && webpack --config webpack.config.js --watch",
        "ui:watch:common": "SET CONFIG_PATH='./env/.common.env' && webpack --config webpack.config.js --watch",
        "ui:devserver": "SET CONFIG_PATH='./env/.devserver.env' && webpack-dev-server --config webpack.config.js",
        "------DB-----": "DB Scripts",
        "db:setup": "start .\\scripts\\setup-and-run.bat",
        "db:run:server": "start cmd /c .\\scripts\\run-db-server.cmd",
        "db:run:client": "start cmd /c .\\scripts\\run-db-client.cmd",
        "db:run:both": "npm run db:run:server && npm run db:run:client",
        "db:export": "start cmd /c .\\scripts\\export-collections.cmd",
        "db:import": "start .\\scripts\\import-db.bat",
        "------SERVER-----": "SERVER Scripts",
        "server:run:clean": "node ./server/bin/www",
        "server:run": "npm run db:run:server && npm run nodemon:server",
        "server:run:common": "SET CONFIG_PATH='./env/.common.env' && npm run db:run:server && npm run nodemon:server",
        "server:run:deploy": "SET CONFIG_PATH='./env/.deploy.env' && npm run node:server",
        "app:run": "npm run ui:watch && npm run server:run",
        "app:run:prod": "SET NODE_ENV=production && npm run ui:build:dev && node ./server/bin/www",
        "app:run:prod:react": "SET NODE_ENV=production && SET APP_TYPE=react && webpack --config configs/webpack/prod.config.js && node ./server/bin/www",
        "app:run:prod:angular": "SET NODE_ENV=production && SET APP_TYPE=angular && webpack --config configs/webpack/prod.config.js && node ./server/bin/www",
        "app:run:prod:native": "SET NODE_ENV=production && SET APP_TYPE=native && webpack --config configs/webpack/prod.config.js && node ./server/bin/www",
        "app:run:prod1": "SET NODE_ENV=production && npm run ui:build:prod && node ./server/bin/www",
        "nodemon:server": "nodemon ./server/bin/www",
        "node:server": "node ./server/bin/www",
        "------TEST-----": "TEST Scripts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "server:run:debug": "SET NODE_ENV=production && nodemon --debug ./server/bin/www",
        "server:run:inspect:prod": "SET NODE_ENV=production && nodemon --inspect ./server/bin/www",
        "server:run:inspect:dev": "SET NODE_ENV=development && nodemon --inspect ./server/bin/www",
        "react:dev": "SET NODE_ENV=development && SET APP_TYPE=react",
        "heroku:deploy": "node ./server/bin/www",
        "postinstall": "./node_modules/.bin/webpack --config webpack.config.js",
        "-----Helper----": "Helper Scripts",
        "clean:modules": "rimraf -- node_modules/**/*"
    },
